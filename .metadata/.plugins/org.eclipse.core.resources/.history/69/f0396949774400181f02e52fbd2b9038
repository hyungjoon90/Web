<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
//location 객체
	function go(a){
		window.location.replace("http://"+a);
	}
	
	function re(){
		window.location.reload();
	}
	
	console.log(location.href); //주소
	console.log(location.protocol);
	console.log(location.host);
	console.log(location.hostname);
	console.log(location.port);
	console.log(location.pathname);
	console.log(location.search);
	
</script>
</head>
<body>
	<button onclick="go('naver.com');">naver</button>
	<button onclick="go('daum.net');">daum</button>
	<button onclick="go('google.com');">google</button>
	<input type="text">
	<button onclick="re();">reload</button>
</body>
</html>